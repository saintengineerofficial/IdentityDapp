{
    "printWidth": 140,
    "bracketSpacing": true,
    "endOfLine": "auto",
    "singleQuote": false,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "wrap_line_length": 130,
    "wrap_attributes": "auto",
    "proseWrap": "always",
    "arrowParens": "avoid",
    "jsxBracketSameLine": true,
    "useTabs": false,
    "eslintIntegration": true,
    "htmlWhitespaceSensitivity": "ignore",
    "plugins": ["prettier-plugin-tailwindcss"]
  }